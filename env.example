# Copy this file to .env and fill in the values

# Telegram Bot
BOT_TOKEN=PASTE_YOUR_TELEGRAM_BOT_TOKEN_HERE

# Google Service Account (choose ONE)
# Path to JSON file on disk (recommended)
GOOGLE_SERVICE_ACCOUNT_JSON=/absolute/path/to/your/service_account.json
# Or raw JSON content (use only if not using file path)
# GOOGLE_SERVICE_ACCOUNT_JSON_CONTENT={"type":"service_account", ...}

# Google Sheets
GOOGLE_SHEET_ID=PASTE_YOUR_SHEET_ID_HERE
GOOGLE_SHEET_RANGE=Applications!A1

# Admin notifications (use one)
# Comma-separated list of admin chat IDs
ADMIN_CHAT_IDS=123456789,987654321
# Or single admin chat ID
# ADMIN_CHAT_ID=123456789
